
public class test {
	public static void main(String[] args) {
		int[][] array = null;
		array[0] = new int[5];
		array[1] = new int[4];
		
		for(int i = 0; i < array.length; i++) {
			for(int j = 0; j < array[i].length; j++) {
				array[i][j] = 100*i+j;
				System.out.println(array[i][j]);
			}
		}
	}
}
